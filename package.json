  "scripts": {
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "start": "cross-env NODE_ENV=test ./node_modules/.bin/webpack serve --config ./config/webpack.config.babel.js --mode none --env development",
    "webpack:watch": "cross-env NODE_ENV=test BABEL_ENV=test ./node_modules/.bin/webpack --config ./config/webpack.config.babel.js --progress --color --watch",
    "webpack:build": "NODE_ENV=development webpack --config ./config/webpack.config.babel.js",
    "webpack:proifile": "NODE_ENV=development webpack --config ./config/webpack.config.babel.js --profile --json",
    "build": "yarn run clean && rollup -c",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn version --patch && yarn publish --non-interactive --access restricted",
    "publish-minor": "yarn version --minor && yarn publish --non-interactive --access restricted",
    "deploy": "firebase deploy",
    "resize": "node --experimental-vm-modules  ./imageMagickResize.js"
  },
